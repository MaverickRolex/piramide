                          <th class="text-end" scope="col">Invitados</th>
                          <th class="text-end" scope="col">Pagos</th>
                          <th class="text-end" scope="col">Por Nivel</th>
                          <th class="text-end" scope="col">Semanal</th>
                          <th class="text-end" scope="col">Quincenal</th>
                          <th class="text-end" scope="col">Mensual</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% @rates.each do |rate| %>
                          <tr>
                            <th scope="row"><%= rate.id %></th>
                            <td class="text-end"><%= rate.guest %></td>
                            <td class="text-end">$<%= rate.payment %></td>
                            <td class="text-end"><%= rate.by_level %></td>
                            <td class="text-end"><%= rate.weekly %></td>
                            <td class="text-end"><%= rate.biweekly %></td>
                            <td class="text-end"><%= rate.monthly %></td>
                          </tr>
                        <% end %>
                      </tbody>
                    </table>
                  </div>
                </div>
                <div class="col-2">
                  <div class="table-responsive">
                    <table class="table">
                      <thead>
                        <tr>
                          <th scope="col">Clasificacion</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% @rates.each do |rate| %>
                          <tr>
